/**index.wxss**/
.search {
  background-color: #ff2d4a;
  padding: 15rpx;
}
.search navigator {
  height: 60rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  color:#999;
  border-radius: 10rpx;
}
.search icon {
  margin-right:20rpx; 
}
swiper {
  width: 750rpx;
  height: 340rpx;
}
swiper image {
  width: 100%;
}
.navs {
  padding: 20rpx 0;
  display: flex;
  justify-content: space-around;
}
.navs image {
  width: 180rpx;
}
.floor {
  padding: 15rpx;
}
.floor_head image {
  width:750rpx;
  height:60rpx;
}
.floor_content {
  display: flex;
}
.content_left {
  width: 232rpx;
  margin-right: 10rpx;
}
.content_left image {
  width: 100%;
}
.content_right {
  flex: 1;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.content_right image {
  width: 233rpx;
}
.floor +.floor .content_right image {
  width: 193rpx;
}
.floor +.floor .content_right image:nth-child(3n+1) {
  width: 273rpx;
}